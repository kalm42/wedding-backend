{
  "name": "eps-backend",
  "version": 1,
  "env": {
    "FRONTEND_URL": "@eps-frontend-url",
    "PRISMA_ENDPOINT": "@eps-prisma-endpoint",
    "PRISMA_SECRET": "@eps-prisma-secret",
    "PRISMA_MANAGEMENT_API_SECRET": "@eps-prisma-management-api-secret",
    "APP_SECRET": "@eps-app-secret",
    "STRIPE_SECRET": "@eps-stripe-secret",
    "PORT": "@port",
    "MAIL_HOST": "@eps-mail-host",
    "MAIL_PORT": "@eps-mail-port",
    "MAIL_USER": "@eps-mail-user",
    "MAIL_PASS": "@eps-mail-pass"
  },
  "alias": "eps-backend.now.sh"
}
